{
  "name": "scheduler-node",
  "version": "1.0.0",
  "description": "Webix Scheduler - example of nodejs backend",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc ",
    "start:dev": "tsc && nodemon ./build/server.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts",
    "migration:run": "npm run typeorm:cli -- migration:run",
    "migration:revert": "npm run typeorm:cli -- migration:revert",
    "migration:generate": "npm run typeorm:cli -- migration:generate -d ./src/migrations/ -n",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed --n ./src/ormconfig.ts"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "webix",
    "ui",
    "scheduler",
    "calendar"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "",
  "dependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/config": "^3.3.1",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^14.14.34",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nodemailer": "^6.9.7",
    "pg": "^8.5.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.2.31",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^5.2.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/nodemailer": "^6.4.14"
  }
}
